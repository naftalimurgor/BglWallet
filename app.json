{
  "expo": {
    "name": "Bitgesell Wallet",
    "slug": "bitgesell-wallet",
    "scheme": "bitgesell-wallet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#FFBA0A"
    },
    "androidStatusBar": {
      "backgroundColor": "#FFBA0A",
      "translucent": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#824FF4"
      },
      "package": "com.naftalimurgor.bitgesellwallet"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e47f78c1-540b-40ba-925e-7ccabe299d23"
      }
    },
    "owner": "nmurgor"
  },
  "web": {
    "bundler": "metro"
  }
}
